<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="custom_fieldset">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="label" xsi:type="string" translate="true">Category Password</item>
                <item name="collapsible" xsi:type="boolean">true</item>
                <item name="sortOrder" xsi:type="number">140</item>
            </item>
        </argument>
        <container name="custom_fieldset_container" >
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">200</item>
                </item>
            </argument>
            <htmlContent name="html_content">
                <argument name="block" xsi:type="object">Sunflowerbiz\CategoryPassword\Block\Adminhtml\Category\Custom</argument>
            </htmlContent>
        </container>
		
        <field name="category_password">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">130</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="label" xsi:type="string" translate="true">Category Password</item>
                </item>
            </argument>
        </field>
		<field name="category_password_changed">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
					<item name="visible" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">131</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="label" xsi:type="string" translate="true"></item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>